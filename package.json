{
    "name": "evmod-utils",
    "displayName": "evmod utils",
    "description": "Syntax highlighting and linting for evmod.* files in evol modules.",
    "version": "0.0.1",
    "publisher": "mo7sen",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "evmod",
            "aliases": ["evolmod", "evmod"],
            "extensions": [".types",".namespaces",".events"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "evmod",
            "scopeName": "source.evmod",
            "path": "./syntaxes/evmod.tmLanguage.json"
        }]
    }
}